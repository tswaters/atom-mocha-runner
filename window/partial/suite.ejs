<li id="<%= uuid %>" class="panel panel-default suite">
  <div class="panel-heading clearfix">
    <h4 class="panel-title" data-toggle="collapse" data-target="#<%= uuid %> > .panel-collapse">
      <div data-uuid="<%= uuid %>" class="pull-left btn-group suite-status">
        <span class="glyphicon glyphicon-refresh spinning"></span>
      </div>
      <%= title %>
      <div class="pull-right btn-group">
        <span class="label label-default">
          <span class="glyphicon glyphicon-time"></span>
          <span data-uuid="<%= uuid %>" class="duration-value">
            <span class="glyphicon glyphicon-refresh spinning"></span>
          </span>
        </span>
        <span class="label label-success">
          <span class="glyphicon glyphicon-ok"></span>
          <span data-uuid="<%= uuid %>" class="passing-value">
            <span class="glyphicon glyphicon-refresh spinning"></span>
          </span>
        </span>
        <span class="label label-danger">
          <span class="glyphicon glyphicon-remove"></span>
          <span data-uuid="<%= uuid %>" class="failing-value">
            <span class="glyphicon glyphicon-refresh spinning"></span>
          </span>
        </span>
        <span class="label label-primary">
          <span class="glyphicon glyphicon-pause"></span>
          <span data-uuid="<%= uuid %>" class="pending-value">
            <%= pending %>
          </span>
        </span>
      </div>
    </h4>
  </div>
  <% if (file !== '') { %>
  <div class="panel-body panel-collapse collapse">
    <%= file %>
  </div>
  <% } %>
  <ul id="<%= uuid %>_tests" class="list-group panel-collapse collapse">
    <% tests.forEach(function (test) { %>
    <li id="<%= test.uuid %>" class="list-group-item test">
      <h5 class="list-group-item-heading">
        <div class="pull-left btn-group">
          <span data-uuid="<%= test.uuid %>" class="result-value">
            <span class="glyphicon glyphicon-refresh spinning"></span>
          </span>
        </div>
        <%= test.title %>
        <div class="pull-right btn-group">
          <span class="label label-default">
            <span class="glyphicon glyphicon-time"></span>
            <span data-uuid="<%= test.uuid %>" class="duration-value">
              <span class="glyphicon glyphicon-refresh spinning"></span>
            </span>
          </span>
          <span class="label label-default" data-toggle="collapse" data-target="#<%= test.uuid %> .code">Code</span>
          <span class="label label-default stack-button" data-toggle="collapse" data-target="#<%= test.uuid %> .error-stack">Stack</span>
        </div>
      </h5>
      <div class="code collapse">
        <pre><code class="hljs"><%- test.code %></code></pre>
      </div>
      <p data-uuid="<%= test.uuid %>" class="error-message text-danger"></p>
      <div data-uuid="<%= test.uuid %>" class="error-stack collapse">
        <pre><code class="hljs"></code></pre>
      </div>
    </li>

    <% }) %>
  </ul>

  <ul class="children list-group panel-collapse collapse"></ul>

</li>
